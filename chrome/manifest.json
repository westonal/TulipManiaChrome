{
  "manifest_version": 2,

  "name": "Tulip Mania",
  "description": "Replaces the word Bitcoin for Tulip",
  "version": "1.0",

  "browser_action": {
    "default_icon": "icon.png"
  },

  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["run.js"]
    }
  ],

  "permissions": [
    "activeTab",
    "storage",
    "https://ajax.googleapis.com/"
  ]
}